    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn prettier-fix"
    ],
    "*.{js,jsx,ts,tsx,md}": "prettier --write"
  }
