{
  "name": "story-front",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "sideEffects": false,
  "scripts": {
    "dev": "dotenv -e .env.local next",
    "build": "next build",
    "start": "cross-env NODE_ENV=production DEPLOY_GROUP=development next start -p 3000",
    "build:dev": "dotenv -e .env.development next build && next export",
    "build:prod": "dotenv -e .env.production next build && next export",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:types": "tsc --noEmit",
    "lint": "next lint",
    "lint:fix": "yarn lint -- --fix",
    "prettier:fix": "prettier --config .prettierrc --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "unused": "next-unused",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OhMinsSup/story-front.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/OhMinsSup/story-front/issues"
  },
  "homepage": "https://github.com/OhMinsSup/story-front#readme",
  "dependencies": {
    "@emotion/babel-plugin": "^11.3.0",
    "@emotion/css": "^11.1.3",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@hookform/resolvers": "^2.8.0",
    "@mui/icons-material": "^5.0.0",
    "@mui/lab": "^5.0.0-alpha.47",
    "@mui/material": "^5.0.0",
    "@multiavatar/multiavatar": "^1.0.6",
    "@prisma/client": "^3.1.1",
    "autoprefixer": "^10.3.6",
    "axios": "^0.22.0",
    "caver-js": "^1.6.4",
    "cross-env": "^7.0.3",
    "filepond": "^4.30.0",
    "filepond-plugin-file-validate-size": "^2.2.5",
    "filepond-plugin-file-validate-type": "^1.2.6",
    "filepond-plugin-get-file": "^1.0.7",
    "filepond-plugin-image-preview": "^4.6.10",
    "filepond-plugin-image-validate-size": "^1.2.6",
    "jsonwebtoken": "^8.5.1",
    "lodash-es": "^4.17.21",
    "next": "^11.1.2",
    "next-compose-plugins": "^2.2.1",
    "next-env": "^1.1.1",
    "next-pwa": "^5.2.24",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.2",
    "react-filepond": "^7.1.1",
    "react-hook-form": "^7.12.2",
    "react-query": "^3.24.5",
    "react-spinners": "^0.11.0",
    "react-use": "^17.3.1",
    "tailwindcss": "^2.2.16",
    "truffle-hdwallet-provider-klaytn": "^1.4.1",
    "yup": "^0.32.9",
    "zustand": "^3.5.12"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^11.0.1",
    "@openzeppelin/contracts": "^4.2.0",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^16.4.10",
    "@types/react": "^17.0.15",
    "@types/react-color": "^3.0.5",
    "@types/yup": "^0.29.13",
    "dotenv-cli": "^4.0.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "prisma": "^3.1.1",
    "typescript": "^4.3.5"
  },
  "gitHooks": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn lint:fix",
      "yarn prettier:fix"
    ],
    "*.{md,json,yml}": [
      "prettier --write"
    ]
  },
  "next-unused": {
    "alias": {
      "@store/*": [
        "src/store/*"
      ],
      "@klaytn/*": [
        "src/klaytn/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@libs/*": [
        "src/libs/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@config/*": [
        "src/config/*"
      ],
      "@constants/*": [
        "src/constants/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@api/*": [
        "src/api/*"
      ]
    },
    "debug": true,
    "include": [
      "src/store",
      "src/klaytn",
      "src/assets",
      "src/libs",
      "src/utils",
      "src/config",
      "src/constants",
      "src/components",
      "src/hooks",
      "src/api",
      "src/pages"
    ],
    "exclude": [],
    "entrypoints": [
      "src/pages"
    ]
  }
}
