{
  "name": "story-front",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "sideEffects": false,
  "engines": {
    "node": ">=14.15.0"
  },
  "scripts": {
    "sitemap": "node --experimental-modules scripts/sitemap.mjs",
    "env": "node --experimental-modules scripts/env.mjs",
    "dev": "cross-env LOAD_ENV=local yarn env && next dev",
    "dev:debug": "cross-env LOAD_ENV=debug yarn env && next dev",
    "build": "next build",
    "start": "cross-env NODE_ENV=production DEPLOY_GROUP=development next start -p 3000",
    "build:analyze": "cross-env LOAD_ENV=analyze yarn env && yarn build",
    "build:dev": "cross-env LOAD_ENV=development yarn env && next build && next export",
    "build:prod": "cross-env LOAD_ENV=production yarn env && next build && next export",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint-types": "tsc --noEmit",
    "lint": "next lint",
    "lint-fix": "next lint --fix",
    "prettier": "prettier --list-different '**/*.{js,jsx,md,ts,tsx}'",
    "prettier-fix": "prettier --write '**/*.{js,jsx,md,ts,tsx}'",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OhMinsSup/story-front.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/OhMinsSup/story-front/issues"
  },
  "homepage": "https://github.com/OhMinsSup/story-front#readme",
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@hookform/resolvers": "^2.8.5",
    "@juggle/resize-observer": "^3.3.1",
    "@mui/icons-material": "^5.2.4",
    "@mui/lab": "^5.0.0-alpha.60",
    "@mui/material": "^5.2.4",
    "@multiavatar/multiavatar": "^1.0.7",
    "autoprefixer": "^10.4.0",
    "axios": "^0.24.0",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.7",
    "lodash-es": "^4.17.21",
    "next": "^12.0.7",
    "next-compose-plugins": "^2.2.1",
    "next-env": "^1.1.1",
    "nookies": "^2.5.2",
    "nprogress": "^0.2.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^7.0.2",
    "qs": "^6.10.2",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.22.2",
    "react-query": "^3.34.5",
    "react-spinners": "^0.11.0",
    "react-sticky-box": "^0.9.3",
    "react-use": "^17.3.1",
    "react-virtualized": "^9.22.3",
    "swiper": "^7.3.3",
    "tailwindcss": "^2.2.19",
    "ua-parser-js": "^1.0.2",
    "yup": "^0.32.11",
    "zustand": "^3.6.7"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.0.7",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^17.0.1",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.37",
    "@types/react-color": "^3.0.6",
    "@types/react-virtualized": "^9.21.15",
    "@types/ua-parser-js": "^0.7.36",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "dotenv": "^10.0.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.0",
    "eslint-config-prettier": "^8.3.0",
    "globby": "^12.0.2",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "postcss": "^8.4.5",
    "prettier": "^2.3.2",
    "shelljs": "^0.8.4",
    "typescript": "^4.5.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn prettier-fix"
    ],
    "*.{js,jsx,ts,tsx,md}": "prettier --write"
  }
}
